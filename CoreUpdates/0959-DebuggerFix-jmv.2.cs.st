lIn: view model ].	aChar == $h ifTrue:[^ self browseHierarchyIn: view model].	aChar == $p ifTrue: [^ self browseFullProtocolIn: view model].	aChar == $N ifTrue: [^ self browseClassRefsIn: view model]! !